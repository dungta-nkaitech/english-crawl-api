  },
  {
    "order": 68,
    "start": 167.759,
    "end": 169.759,
    "text": "will stimulate your brain.",
    "speaker": null
  },
  {
    "order": 69,
    "start": 169.759,
    "end": 172.56,
    "text": "Or getting dressed in the dark with your eyes shut,",
    "speaker": null
  },
  {
    "order": 70,
    "start": 172.56,
    "end": 175.359,
