    "speaker": null
  },
  {
    "order": 74,
    "start": 182.479,
    "end": 183.439,
    "text": "Okay, let's see.",
    "speaker": null
  },
  {
    "order": 75,
    "start": 184.319,
    "end": 185.439,
    "text": "Pizza.",
    "speaker": null
  },
  {
    "order": 76,
